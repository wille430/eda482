.PHONY: clean All

All:
	@echo "----------Building project:[ asciidisplay - Debug ]----------"
	@cd "asciidisplay" && "$(MAKE)" -f  "asciidisplay.mk" && "$(MAKE)" -f  "asciidisplay.mk" PostBuild
clean:
	@echo "----------Cleaning project:[ asciidisplay - Debug ]----------"
	@cd "asciidisplay" && "$(MAKE)" -f  "asciidisplay.mk" clean
